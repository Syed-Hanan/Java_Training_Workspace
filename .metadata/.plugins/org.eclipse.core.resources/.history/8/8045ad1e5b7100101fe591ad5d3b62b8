package core;

import java.util.ArrayList;
import java.util.Scanner;

public class StudentResults {
	public static void main(String[] args)
	{
		ArrayList<Student> list=new ArrayList<Student>();
		Scanner sc= new Scanner(System.in);
		
		System.out.println("enter no");
		int number=sc.nextInt();
		System.out.println("number "+" "+number);
		
		list.add(new Student(sc.nextInt(),sc.next(),sc.nextDouble(),sc.nextDouble(),sc.nextDouble()));
		list.add(new Student(sc.nextInt(),sc.next(),sc.nextDouble(),sc.nextDouble(),sc.nextDouble()));
		list.add(new Student(sc.nextInt(),sc.next(),sc.nextDouble(),sc.nextDouble(),sc.nextDouble()));
		
		int i=0;
		for(Student st :list) {
			/*double a = st.getSub1marks()+getSub2marks()+st.getSub3marks()
			System.out.println(st.getSub1marks()+" "+st.getSub2marks()+" "+st.getSub3marks());
		double total = st.getSub1marks()+st.getSub2marks()+st.getSub3marks();
		
		double avg =total/3;
		if(avg>40)
		{
			System.out.println("pass");
		}
		else {
		System.out.println("fail");
		}*/
			
			double marks=st.getMarks();
			double avg = marks/3;
			if(avg>40)
			{
				System.out.println("pass");
			}
			else {
			System.out.println("fail");
			}
	i++;
		}
	}

}
