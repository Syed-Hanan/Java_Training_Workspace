package core;

public class CheckDaysInmonth {
	
	public enum Month {
		JAN(31),
		FEB(28),
		MAR(31),
		APR(30),
		MAY(31),
		JUN(30),
		JUL(31),
		AUG(30),
		SEP(31),
		OCT(30),
		NON(31),
		DEC(30);
		
		private final int days;
		
		Month(int days)
		{
			this.days=days;
		}
	}

	public static void main(String[] args) {
		
   for (Month month:Month.values())
   {
	   System.out.println(month);
   }
	}

}
