<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
    
    <bean id="std" class="springproject.Student"> 
    
      <constructor-arg name="regno" value="404"></constructor-arg>
      <constructor-arg name="Stdname" value="hey"></constructor-arg>
   
    </bean> 
    
    <bean id="two" class="springproject.Employee"> 
    <property name="code" value="345"></property>
    <property name="empname" value="hanan"></property>
    <property name="salary" value="4890"></property>
    <property name="addr" ref="address"></property>
    
    </bean> 
    
    <bean id="address" class="springproject.AddressOfEmployee"> 
    <property name="city" value="kashmir"></property>
    
    
    </bean> 
    
    
    
</beans>
